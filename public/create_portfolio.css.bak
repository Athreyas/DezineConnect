#wrap {
    margin-top: -10px;
  /*background: #fff;*/
}
#main {
    overflow: visible;
    float: left;
    /*background: gray;*/
  
    width: 635px;
    background: none;
}
/*common for portfolio*/
.error_msg {
    color: red;
    margin: 5px 0;
    display:none;
    margin-left: 10px;
    float: left;
}

#portfolio_sider {
    margin: 55px 0 0 13px;
    font-size: 13px;
}
.portfolio_reg_steps, .portfolio_reg_steps_select, .portfolio_regsteps_selected_after{
  /* position: absolute;
     top:0;
    right:-300px;*/
    -moz-border-radius-topleft:5px;
    -moz-border-radius-topright:5px;
    -moz-border-radius-bottomleft:5px;
    -moz-border-radius-bottomright:5px;

    -webkit-border-top-left-radius:5px;
    -webkit-border-top-right-radius:5px;
    -webkit-border-bottom-left-radius:5px;
    -webkit-border-bottom-right-radius:5px;

    width: 220px;
}
.portfolio_reg_steps {
    background-color:gray;
    color:#fff;
}
.portfolio_reg_steps_select{
    background-color:#f78f1e;
    color:#fff;
}
.portfolio_reg_steps_in_select, .portfolio_reg_steps_in {
    margin: 10px 0 0 -11px;
    padding: 10px 20px 3px 20px;
    height: 25px;
}
.portfolio_reg_steps_in_select {
    background: url(../images/tipsy-west.png) no-repeat;
    background-position:left center;
    color: black;
}
.portfolio_regsteps_selected_after {
    background: #ffcc99;
    color: gray;
}
/*common for portfolio end css*/
/*reguler css*/
#main_inner label {
    font-size: 13px;
    float: left;
    width: 19em;
    clear: left;
    margin: 6px 0px 5px 0px;
    color: #666666;
}

#main_inner label a{
    color: orange;
    float: right;
    }

#main_inner fieldset {
    border: 0;
    margin: 10px 0;
    padding: 0px;
}
#chk1 {
    margin: 10px 0 0 0;
    padding: 0;
    vertical-align: middle;
    height: 18px;
}
#chk1 input {
    border: 0;
}
#main_inner select {
    float: left;
    height: 28px;
    border:1px solid #bbbbbb;
    border-style:  solid;
    border-width: 1px;
    padding:5px 3px;
    margin: 0 5px 10px 0;
}
/*reguler css end*/


#register_proceed_btn button {
    color: #000;
    margin: 5px 0 0 0;
    background: url('/images/register&proceed.png') no-repeat 0px 0px;
    height: 30px;
    width: 150px;
    cursor: pointer;
}
#register_proceed_btn button:hover {
    color: #000;
    background: url('/images/register&proceed_hover.png') no-repeat 0px 0px;
}




/*portfolio*/
#accountsetting_socialinfo {
    float: left;
    margin: 10px 0 0 0;
    position: relative;
}
#accountsetting_socialinfo h4{
    margin-left: 10px;
}
#accountsetting_socialinfo div {
    margin: 38px 0 0 0;
}
#accountsetting_socialinfo div div{
    height: 30px;
}
#accountsetting_socialinfo label{
    margin: 10px 0 10px 0;
}
#accountsetting_socialinfo input{
   position: absolute;
   left: 50px;
}
#accountsetting_socialinfo img {
   position: absolute;
   margin: 2px 0 0 10px;
   height: 32px;
}
#main_inner select{
    height: 25px;
}

br {
    clear: both;
}
#portfolio_msg_text {
    color: gray;
    clear: both;
}
#main_inner textarea {
    border: 1px solid #ccc;
    width: 450px;
}
#main_inner h5 {
    margin: 5px 0 10px 0;
}
.portfolio_rounded_shading {
    background: #fff;
    -moz-border-radius-topleft:10px;
    -moz-border-radius-topright:10px;
    -moz-border-radius-bottomleft:10px;
    -moz-border-radius-bottomright:10px;

    -webkit-border-top-left-radius:10px;
    -webkit-border-top-right-radius:10px;
    -webkit-border-bottom-left-radius:10px;
    -webkit-border-bottom-right-radius:10px;
    width: 600px;
    margin: 0px 0px 0px 0px;
    padding: 5px;
    border-bottom: 2px solid #ccc;
    border-right: 2px solid #ccc;
    float: left;
    position: relative;
}
#main_inner .portfolio_rounded_shading input {
    width: 200px;
}
#portfolio_publish_btn {
    width: 151px;
    background: url('../images/publish.png') no-repeat;
    height: 31px;
}
#main_inner #portfolio_wordCount {
    width: 100px;
}